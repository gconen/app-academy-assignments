<h1>StaticPages#contact</h1>
  <div class="row">
      <div class="col-xs-5 col-xs-offset-1">
        <form action="/session" method="POST">
          <input
            type="hidden"
            name="authenticity_token"
            value="<%= form_authenticity_token %>">
          <div class="form-group">
          <label for="user_username">Username</label>
          <input type="text" name="user[username]" id="user_username" class="form-control">
          </div>
          <div class="form-group">
          <label for="user_password">Password</label>
          <input type="password" name="user[password]" id="user_password" class="form-control">
          </div>
          <input type="button" class="btn btn-primary" value="Log in!">
        </form>
      </div>
    <div class="col-xs-5">
      <table class="table table-striped">
        <tr>
          <td>Alice</td>
          <td>Lastname</td>
          <td>555-5555</td>
        </tr>
        <tr>
          <td>Bob</td>
          <td>Lastname</td>
          <td>555-5556</td>
        </tr>
        <tr>
          <td>Carol</td>
          <td>Lastname</td>
          <td>555-5557</td>
        </tr>
      </table>
    </div>
  </div>
